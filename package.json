        "@types/react-dom": "^18",
        "autoprefixer": "^10.4.17",
        "clsx": "^2.1.0",
        "eslint": "^8",
        "eslint-config-next": "14.1.0",
        "lucide-react": "^0.344.0",
        "next": "14.1.0",
        "openai": "^4.28.0",
        "postcss": "^8",
        "react": "^18",
        "react-dom": "^18",
        "react-markdown": "^10.1.0",
        "recharts": "^2.12.0",
        "rehype-highlight": "^7.0.2",
